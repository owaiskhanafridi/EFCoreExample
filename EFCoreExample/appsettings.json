{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "Serilog": {
      "MinimumLevel": "Information",
      "WriteTo": [ { "Name": "Console" } ],
      "Enrich": [ "FromLogContext" ]
    }
  ,
  "Swagger": {
    "Enable": true
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "Server=localhost\\SQLEXPRESS; Database=AmazonDB; Trusted_Connection=True; TrustServerCertificate=True"
  }
}
